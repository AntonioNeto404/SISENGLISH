{
    "name": "siscap03/project",
    "description": "SISCAP Sistema de Capacitação",
    "require": {
        "vlucas/phpdotenv": "^5.5",
        "monolog/monolog": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "classmap": [
            "models/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "fix": "php-cs-fixer fix"
    }
}